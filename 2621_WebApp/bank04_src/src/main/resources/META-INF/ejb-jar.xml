<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
	version="3.2">
	
	<enterprise-beans>
		<session>
			<ejb-name>AccountService</ejb-name>
			<env-entry>
				<env-entry-name>withdrawLimit</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>1000</env-entry-value>
			</env-entry>
		</session>
		<session>
			<ejb-name>AccountManager</ejb-name>
			<env-entry>
				<env-entry-name>withdrawLimit</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>1000</env-entry-value>
			</env-entry>
		</session>
		<session>
			<ejb-name>InterestService</ejb-name>
			<env-entry>
				<env-entry-name>interestRate</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>0.05</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
	

</ejb-jar>
